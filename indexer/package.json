{
  "name": "squid",
  "private": true,
  "version": "0.0.1",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "rm -rf lib && tsc",
    "run": "node lib/api.js",
    "processor": "node lib/main.js",
    "migration:generate": "yarn build && npx typeorm migration:generate -d lib/model/dataSource.js -o db/migrations/$NAME",
    "migration:run": "typeorm migration:run -d lib/model/dataSource.js",
    "serve": "node lib/api.js"
  },
  "dependencies": {
    "@gear-js/api": "^0.43.0",
    "@polkadot/api": "^16.4.3",
    "@polkadot/api-augment": "^16.4.3",
    "@subsquid/substrate-processor": "^8.6.0",
    "@subsquid/substrate-runtime": "^2.0.1",
    "@subsquid/typeorm-store": "^1.5.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "pg": "8.12.0",
    "postgraphile": "^4.14.1",
    "postgraphile-plugin-connection-filter": "^2.3.0",
    "reflect-metadata": "^0.2.2",
    "sails-js": "^0.4.2",
    "sails-js-parser": "^0.4.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@subsquid/substrate-metadata-explorer": "^3.1.2",
    "@types/node": "^20.14.8",
    "typescript": "^5.5.2"
  }
}
